{
  "name": "flight-routes-schedule",
  "version": "1.0.0",
  "description": "Flight schedule generator for flight simulation",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "DEBUG_PRINT_LIMIT=100000 jest",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "test-client": "ts-node tests/testClient.ts",
    "test:unit": "DEBUG_PRINT_LIMIT=100000 jest tests/unit",
    "test:integration": "DEBUG_PRINT_LIMIT=100000 jest tests/integration",
    "test:watch": "DEBUG_PRINT_LIMIT=100000 jest --watch"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
